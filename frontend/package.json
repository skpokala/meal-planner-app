{
  "name": "meal-planner-frontend",
  "version": "1.1.41",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.4",
    "recharts": "^2.8.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-dom": "^18.2.7",
    "@types/react-modal": "^3.16.0",
    "autoprefixer": "^10.4.14",
    "jest-junit": "^16.0.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "npm run test:ci && REACT_APP_VERSION=$npm_package_version react-scripts build",
    "build:ci": "npm run test:ci && CI=false REACT_APP_VERSION=$npm_package_version react-scripts build",
    "build:skip-tests": "CI=false REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=false react-scripts test --coverage --ci --watchAll=false --passWithNoTests",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js",
      "!src/setupTests.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/*.{test,spec}.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|react-router-dom|@testing-library)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
